{
  "scenarioName": "LocksLastUsed",
  "request": {
    "method": "PUT",
    "urlPathPattern": "/survey/api/survey/([a-zA-Z0-9/-]*)/locks/([a-zA-Z0-9/-]*)/lastUsed",
    "headers": {
      "Authorization": {
        "matches": "Bearer ([A-Za-z0-9-_=]+\\.[A-Za-z0-9-_=]+\\.?[A-Za-z0-9-_.+/=]*)"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "id": "{{request.path.[5]}}",
      "lastUsedAt": "{{now}}",
      "status": "IN_PROGRESS",
      "userId": "{{regexExtract request.headers.Authorization.[0] 'Bearer +[A-Za-z0-9-_=]+.([A-Za-z0-9-_=]+)' 'groups'}}{{#assign 'payload'}}{{base64 groups.[0] decode=true}}{{/assign}}{{jsonPath payload '$.id'}}",
      "newLock": true
    },
    "fixedDelayMilliseconds": 100
  }
}